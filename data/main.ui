<interface>
  <object id="main-window" class="GtkApplicationWindow">
    <property name="title">Lumee</property>
    <property name="default-width">800</property>
    <property name="default-height">500</property>
    <property name="window-position">GTK_WIN_POS_CENTER</property>
    <child type="titlebar">
      <object id="header-bar" class="GtkHeaderBar">
        <property name="show-close-button">true</property>
      </object>
    </child>
    <child>
      <object id="grid" class="GtkGrid">
        <child>
          <object id="scrolled-window" class="GtkScrolledWindow">
            <property name="min-content-width">119</property>
            <child>
              <object id="file-list" class="GtkTreeView">
                <property name="name">file-list</property>
                <property name="vexpand">true</property>
                <property name="headers-visible">false</property>
                <child internal-child="selection">
                  <object id="file-list-selection" class="GtkTreeSelection">
                    <property name="mode">GTK_SELECTION_BROWSE</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object id="scrolled-window-2" class="GtkScrolledWindow">
            <child>
              <object id="image" class="GtkImage">
                <property name="hexpand">true</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
